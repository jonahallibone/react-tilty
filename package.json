{
  "name": "react-tilty",
  "version": "2.0.3",
  "description": "A React port of the JS component tilt.js",
  "source": "src/index.js",
  "main": "dist/react-tilty.js",
  "module": "dist/react-tilty.esm.js",
  "exports": "./dist/react-tilty.modern.js",
  "unpkg": "dist/react-tilty.umd.js",
  "scripts": {
    "build": "microbundle build --jsx React.createElement",
    "start": "concurrently \"npm:dev*\"",
    "dev:component": "microbundle watch --jsx React.createElement",
    "dev:example": "(cd example && npm start)",
    "setup": "npm i && npm link && cd example && npm i && npm link react-tilty",
    "prepublishOnly": "npm run build"
  },
  "author": {
    "name": "Jonah Allibone"
  },
  "contributors": [
    "Chris Sandvik"
  ],
  "keywords": [
    "react",
    "component",
    "tilt",
    "tilty",
    "tilt.js",
    "tilty.js",
    "vanilla-tilt",
    "react.js",
    "react-tilt",
    "react-tilty",
    "react-tilt.js"
  ],
  "devDependencies": {
    "concurrently": "^6.0.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "microbundle": "^0.13.0",
    "prettier": "^2.2.1",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonahallibone/react-tilty"
  },
  "homepage": "https://codesandbox.io/s/73rqoq599j",
  "license": "MIT",
  "files": [
    "dist"
  ]
}
